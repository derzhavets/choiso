<% current_user.own_alternatives.each do |alternative| %>
  <thead><tr><td><h4><%= alternative.name.capitalize %></h4></td></tr></thead>
  <tbody>
    <% alternative.critical_points.proposed_by(@proposal.proposer).each do |cp| %>
      <tr><td><%= cp.point.name %></td></tr>
    <% end %>
  </tbody>
<% end %>