<div class="row">
  <div class="col-md-12">
    <%= render 'list', :showable => "strengths" %>
  </div>
  <div class="col-md-12">
    <%= render 'list', :showable => "weaknesses" %>
  </div>
</div>

<%= content_for :right_menu do %>
  <%= render 'shared/proposals', :showable => "strengths" %>
  <span id="contacts-listing">
    <%= render partial: 'requests/collectible', object: current_user.strengths.first %>
  </span>
<% end %>